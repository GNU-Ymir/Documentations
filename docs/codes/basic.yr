def main () {
    // We create a float with a default value
    let a = float::init;
    
    // Variable types are infered
    let b = 12; // b is an int

    // A suffix can be used to specify the type
    // Here c is an uint, d a bool and, e a char
    let c = 12U, d = false, e = 'r';

    // Error, cannot change type of variables.
    c = false;
}
